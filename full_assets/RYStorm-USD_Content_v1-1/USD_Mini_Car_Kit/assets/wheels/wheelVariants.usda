            )
            {
            }

        }
        "wheelRed" {
            def "wheelRedAsset" (
                prepend references = @./wheelRed/asset/wheelRedAsset.usda@</wheelRed>
            )
            {
            }

        }
        "wheelVintage" {
            def "wheelVintageAsset" (
                prepend references = @./wheelVintage/asset/wheelVintageAsset.usda@</wheelVintage>
            )
            {
            }

        }
        "wheelWide" {
            def "wheelWideAsset" (
                prepend references = @./wheelWide/asset/wheelWideAsset.usda@</wheelWide>
